
//URL" https://leetcode.com/problems/how-many-numbers-are-smaller-than-the-current-number/

function smallerNumber(nums) {
  const result = []

  nums.forEach((num, index) => {
    let count = 0
    nums.forEach((innerNum, innerIndex) => {
      if (innerIndex !== index && innerNum < num) {
        count++
      }
    })
    result.push(count)
  })

  return result
}



