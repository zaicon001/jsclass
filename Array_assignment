   // 3. https://leetcode.com/problems/kids-with-the-greatest-number-of-candies/

    function kidsWCandies(candies, extraCandies) {
        const maxCandies = Math.max(...candies)

        return candies.map(candy => candy + extraCandies >= maxCandies)
    }